import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);

        // Create two 3x3 matrices
        int[][] matrix1 = new int[3][3];
        int[][] matrix2 = new int[3][3];

        // values for first matrix
        System.out.println("Enter values for the first 3x3 matrix:");
        readMatrix(matrix1, input);

        //values fo second matrix
        System.out.println("Enter values for the second 3x3 matrix:");
        readMatrix(matrix2, input);

        // Are the matrices are identical
        if (areMatricesEqual(matrix1, matrix2)) {
            System.out.println("The two matrices are identical.");
        } else {
            System.out.println("The two matrices are not identical.");
        }

        input.close();
    }

    //value into a matrix
    public static void readMatrix(int[][] matrix, Scanner input) {
        for (int i = 0; i < matrix.length; i++) {
            for (int j = 0; j < matrix[i].length; j++) {
                matrix[i][j] = input.nextInt();
            }
        }
    }

    // Check if two matrices are identical or not
    public static boolean areMatricesEqual(int[][] matrix1, int[][] matrix2) {
        for (int i = 0; i < matrix1.length; i++) {
            for (int j = 0; j < matrix1[i].length; j++) {
                if (matrix1[i][j] != matrix2[i][j]) {
                    return false;
                }
            }
        }
        return true;
    }
}
